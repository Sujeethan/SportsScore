<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raghavendra.sportslivescore" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Videos.CricketVideos"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_video_list" >
            <meta-data
                android:name="@string/minVersion"
                android:value="13" />
            <meta-data
                android:name="@string/isLaunchableActivity"
                android:value="true" />
        </activity>
        <activity
            android:name=".Videos.SoccerVideos"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_video_list2" >
            <meta-data
                android:name="@string/minVersion"
                android:value="13" />
            <meta-data
                android:name="@string/isLaunchableActivity"
                android:value="true" />
        </activity>
        <activity
            android:name=".Videos.TennisVideos"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_video_list3" >
            <meta-data
                android:name="@string/minVersion"
                android:value="13" />
            <meta-data
                android:name="@string/isLaunchableActivity"
                android:value="true" />
        </activity>
        <activity
            android:name=".Videos.YouTubeAPIActivity"
            android:label="@string/title_activity_you_tube_api" >
            <meta-data
                android:name="@string/isLaunchableActivity"
                android:value="false" />
        </activity>

    </application>

</manifest>
